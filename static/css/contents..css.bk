/*레이어팝업*/
.datailInfo_AIDI{height:100%;margin:0 -1.875rem}
.datailInfo_AIDI:after{clear:both;display:block;content:""}
.datailInfo_AIDI > [class*="datailInfo_"]{float:left;position:relative;width:50%;height:100%;padding:2.625rem 1.875rem 0}
.datailInfo_AIDI > [class*="datailInfo_"] > strong{position:absolute;top:0;left:1.875rem;font-weight:500;font-size:1.375rem;line-height:1.875rem;color:#fff;letter-spacing:-0.046875rem}
.datailInfo_AIDI .AI_wrap,.datailInfo_AIDI .DI_wrap{display:block;height:100%;padding:2.1875rem;border:1px solid #3b3e47;border-radius:0.3125rem}
.datailInfo_AIDI .data_table td > span{margin-left:1.875rem}
.datailInfo_AIDI .data_table td > .mark{display:inline-block;float:right;position:static;margin:0 1.25rem 0 0;line-height:1.25rem;vertical-align:top}
.datailInfo_AIDI .data_table td > .mark > span{display:block;float:none;line-height:1.25rem}
.datailInfo_AIDI .data_table td > .mark .icon{display:block;float:none;margin:0.1875rem}
.datailInfo_AIDI .mark{position:absolute;top:0;right:1.875rem;line-height:1.8125rem;color:#fff}
.datailInfo_AIDI .mark > span{float:left;line-height:1.8125rem;vertical-align:top}
.datailInfo_AIDI .mark > span + span{margin-left:0.3125rem}
.datailInfo_AIDI .mark .icon{float:left;position:relative;overflow:hidden;width:0.875rem;height:0.875rem;margin:0.46875rem 0.3125rem;border-radius:50%}
.datailInfo_AIDI .mark .on .icon{background:#26ceb4}
.datailInfo_AIDI .mark .off .icon{background:#ff7387}